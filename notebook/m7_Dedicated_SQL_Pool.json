				"source": [
					"val sqlPool = f\"SQL_pool_low_performance.m7.TaxiDemoAggregated_$notebook\"\r\n",
					"\r\n",
					"// INTO DEDICATED SQL POOL\r\n",
					"val sql_df = spark.sqlContext.sql(\"SELECT * FROM TaxiDemoAgg\")\r\n",
					"sql_df.write.sqlanalytics(sqlPool, Constants.INTERNAL)\r\n",
					""
				],
